{
  "name": "amchi-mumbai-ai",
  "version": "1.0.0",
  "description": "Local guide application for Mumbai tourists",
  "main": "dist/server/server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon --exec ts-node src/server/index.ts",
    "dev:client": "cd src/client && npm start",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "cd src/client && npm run build",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "deploy:gh-pages": "cd src/client && npm run build && cd ../.. && cp -r src/client/build/* . && git add . && git commit -m \"Deploy to GitHub Pages\" && git push",
    "start": "node dist/server/index.js",
    "start:prod": "cross-env NODE_ENV=production node dist/server/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "postinstall": "cd src/client && npm install"
  },
  "keywords": [
    "mumbai",
    "tourism",
    "local-guide",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^6.0.0",
    "@types/express-session": "^1.17.10",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "rimraf": "^6.1.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "fast-check": "^3.15.0",
    "helmet": "^8.1.0",
    "mongodb": "^6.3.0",
    "node-fetch": "^3.3.2",
    "redis": "^4.6.10"
  }
}
